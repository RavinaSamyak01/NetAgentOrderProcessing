<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="NetAgentSmokeTest">


	<!--Reports Tab -->
	<test thread-count="5" name="Agent Activity">
		<classes>
			<class name="netAgent_Reports.AgentActivity"></class>
		</classes>
	</test>
	<test thread-count="5" name="Agent Activity Chart">
		<classes>
			<class name="netAgent_Reports.AgentActivityChart"></class>
		</classes>
	</test>
	<test thread-count="5" name="Agent Activity Detail">
		<classes>
			<class name="netAgent_Reports.AgentActivityDetail"></class>
		</classes>
	</test>
	<test thread-count="5" name="Quarantine Report">
